{
  "name": "app_infra",
  "version": "0.0.0",
  "description": "app_infra module for mf-webpack",
  "scripts": {
    "husky": "run-s husky:install husky:pre-commit",
    "husky:install": "husky install",
    "husky:pre-commit": "npx husky add .husky/pre-commit \"npm run pre-commit\"",
    "pre-commit": "npx lint-staged",
    "check:code": "tsc --noEmit",
    "lint:code": "eslint ./src --ext .ts,.tsx",
    "lint:codefix": "eslint ./src --ext .ts,.tsx --fix",
    "clean": "rimraf dist",
    "build:dev": "webpack --config webpack/dev.config.js",
    "build:prod": "webpack --config webpack/prod.config.js",
    "test": "jest",
    "server": "node server.js",
    "start": "webpack serve -c webpack/dev.config.js",
    "api": "json-server --watch ./json-server/db.json --port 3005 --no-cors"
  },
  "engines": {
    "node": ">=14"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix"
    ],
    "/src/.*\\.(test|spec)?\\.(ts|tsx)$": [
      "test"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.21.0",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@types/mini-css-extract-plugin": "2.5.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-html-reporter": "3.7.0",
    "jest-watch-typeahead": "2.2.2",
    "json-server": "0.17.3",
    "lint-staged": "13.1.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.4",
    "react-refresh": "0.14.0",
    "react-refresh-typescript": "2.0.8",
    "ts-jest": "29.0.5",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "@antv/x6": "2.0.7",
    "@antv/x6-plugin-selection": "2.1.5",
    "@antv/x6-react-shape": "2.0.7",
    "@consta/uikit": "4.13.0",
    "axios": "1.3.4",
    "babel-loader": "9.1.2",
    "css-loader": "6.7.3",
    "dotenv": "16.0.3",
    "dotenv-webpack": "8.0.1",
    "highcharts": "9.2.2",
    "highcharts-react-official": "3.1.0",
    "html-webpack-plugin": "5.5.0",
    "mini-css-extract-plugin": "2.7.2",
    "mobx": "6.8.0",
    "mobx-react-lite": "3.4.0",
    "path": "0.12.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.8.2",
    "rimraf": "4.3.0",
    "sass": "1.58.3",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "type-fest": "3.6.1",
    "typescript": "4.9.5",
    "webpack": "^5.77.0",
    "webpack-cli": "5.0.1"
  }
}
